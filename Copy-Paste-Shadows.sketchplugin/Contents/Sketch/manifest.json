{
  "name": "Copy Paste Shadows",
  "description": "An utility plugin to copy paste shadows across layers",
  "author": "Arth K. Gajjar",
  "identifier": "com.iarthstar.copy-paste-shadows",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Copy Shadows",
      "identifier": "copy-paste-shadows.copy-shadows-identifier",
      "shortcut": "ctrl shift c",
      "script": "copy-shadows.js"
    },
    {
      "name": "Paste Shadows",
      "identifier": "copy-paste-shadows.paste-shadows-identifier",
      "shortcut": "ctrl shift v",
      "script": "paste-shadows.js"
    }
  ],
  "menu": {
    "title": "Copy Paste Shadows",
    "items": [
      "copy-paste-shadows.copy-shadows-identifier",
      "copy-paste-shadows.paste-shadows-identifier"
    ]
  },
  "version": "1.1.0",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/iarthstar/copy-paste-shadows/master/.appcast.xml"
}